

{% extends 'base.jinja' %}

{% block content %}
<h1>{{ sport_event.name }}</h1>
<h2>{{ sport_event.sport }}</h2>
<h2>{{ sport_event.location}}</h2>
<h2>{{ sport_event.date }}</h2>

<h3>Attended by:</h3>

<ul>
    {% for user in users %}
    <li>{{ user.name }}</li>
    {% endfor %}
</ul>

<a href="/sportevents/{{ sport_event.id }}/edit">Edit</a>

{% endblock %}


